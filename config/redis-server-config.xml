	<!-- redis获取Jedis最大等待时间-毫秒 -->
	<redisMaxWait>3000</redisMaxWait>
	<!-- redis获取连接检查有效性 -->
	<redisTestOnBorrow>true</redisTestOnBorrow>
	<!-- redis释放连接检查有效性-->
